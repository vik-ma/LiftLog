<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="LocalLiftLog.Pages.SetListDetailsPage"
             xmlns:viewmodel="clr-namespace:LocalLiftLog.ViewModels"
             xmlns:models="clr-namespace:LocalLiftLog.Models"
             x:DataType="viewmodel:SetListDetailsViewModel"
             Title="Set List Details Page">
    <VerticalStackLayout>
        <HorizontalStackLayout
            HorizontalOptions="Center"
            Spacing="5">
            <Label 
                Text="Set Template Collection Id:"
                FontSize="30"
                FontAttributes="Bold" />
            <Label 
                Text="{Binding SetTemplateCollection.Id}"
                FontSize="30"
                FontAttributes="Bold" />
        </HorizontalStackLayout>
        <Button 
            Text="Go Back"
            Style="{StaticResource ProgramButtonStyle}"
            Command="{Binding GoBackCommand}"
            Margin="50" />
    </VerticalStackLayout>
</ContentPage>